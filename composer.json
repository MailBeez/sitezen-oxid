{
  "name": "sitezen/sitezen-oxid",
  "description": "Plugin for Sitezen.io",
  "type": "oxideshop-module",
  "keywords": [
      "oxid",
      "modules",
      "eShop",
      "sitezen"
  ],
  "homepage": "https://sitezen.io",
  "license": [
    "GPL-3.0-only"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "prefer-dist": true,
  "require": {
    "php": ">=8.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "SiteZen\\SiteZenOxid\\": "src/"
    }
  },
  "config": {
      "allow-plugins": {
          "oxid-esales/oxideshop-composer-plugin": true,
          "oxid-esales/oxideshop-unified-namespace-generator": true
      }
  }
}
